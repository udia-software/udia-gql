{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": false,
        "object-literal-sort-keys": false,
        "no-implicit-dependencies": [true, "optional"],
        "semicolon": [true, "always", "ignore-bound-class-methods"],
        "trailing-comma": false,
        "quotemark": true
    },
    "rulesDirectory": []
}