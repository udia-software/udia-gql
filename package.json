{
  "name": "udia-gql",
  "version": "0.0.0",
  "description": "Serverless GraphQL endpoint for Udia",
  "main": "handler.ts",
  "author": {
    "name": "Alexander Wong",
    "email": "alex@udia.ca",
    "url": "https://udia.ca"
  },
  "scripts": {
    "start": "serverless offline start",
    "test": "jest",
    "posttest": "npm run lint",
    "lint": "tslint --project . --config tslint.json",
    "debug": "SLS_DEBUG=* node --inspect node_modules/.bin/serverless offline --stage dev",
    "load_dev_db": "ts-node src/util/initDynamoDB.ts",
    "deploy": "serverless deploy"
  },
  "dependencies": {
    "apollo-server-core": "^2.5.0",
    "apollo-server-lambda": "^2.5.0",
    "argon2": "^0.23.0",
    "aws-lambda": "^0.1.2",
    "aws-sdk": "^2.464.0",
    "graphql": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "pbkdf2": "^3.0.17",
    "tweetnacl": "^1.0.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.26",
    "@types/graphql": "^14.2.0",
    "@types/jest": "^24.0.13",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/node": "8.10.x",
    "@types/pbkdf2": "^3.0.0",
    "@types/uuid": "^3.4.4",
    "jest": "^24.8.0",
    "serverless": "^1.44.1",
    "serverless-offline": "^4.10.6",
    "serverless-plugin-typescript": "^1.1.7",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "tslint": "^5.16.0",
    "typescript": "^3.5.1",
    "typescript-tslint-plugin": "^0.4.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/udia-software/udia-gql.git"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "handler.ts",
      "src/**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!src/util/initDynamoDB.ts"
    ],
    "coverageDirectory": "coverage"
  },
  "license": "Apache-2.0"
}
