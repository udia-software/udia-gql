{
  "name": "udia-gql",
  "version": "0.0.0",
  "description": "Serverless GraphQL endpoint for Udia",
  "main": "src/handler.ts",
  "author": {
    "name": "Alexander Wong",
    "email": "alex@udia.ca",
    "url": "https://udia.ca"
  },
  "scripts": {
    "start": "serverless offline start",
    "test": "jest --verbose",
    "posttest": "npm run lint",
    "lint": "tslint --project . --config tslint.json",
    "debug": "SLS_DEBUG=* node --inspect node_modules/.bin/serverless offline --stage dev",
    "load_dev_db": "ts-node src/util/initDynamoDB.ts",
    "deploy": "serverless deploy"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.2",
    "apollo-link-http": "^1.5.14",
    "apollo-server-core": "^2.6.2",
    "apollo-server-lambda": "^2.6.2",
    "argon2": "^0.23.0",
    "aws-lambda": "^0.1.2",
    "aws-sdk": "^2.472.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "history": "^4.9.0",
    "jsonwebtoken": "^8.5.1",
    "katex": "^0.10.2",
    "pbkdf2": "^3.0.17",
    "react": "^16.8.6",
    "react-apollo": "^2.5.6",
    "react-dom": "^16.8.6",
    "react-helmet-async": "^1.0.2",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-transition-group": "^4.1.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "serverless-http": "^2.0.2",
    "stream-inject": "^1.0.2",
    "styled-components": "^4.3.1",
    "tweetnacl": "^1.0.1",
    "unfetch": "^4.1.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@types/aws-lambda": "^8.10.27",
    "@types/aws-serverless-express": "^3.3.1",
    "@types/cors": "^2.8.5",
    "@types/graphql": "^14.2.0",
    "@types/jest": "^24.0.13",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/katex": "^0.10.1",
    "@types/node": "^8.10.49",
    "@types/node-fetch": "^2.3.5",
    "@types/pbkdf2": "^3.0.0",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/react-redux": "^7.0.9",
    "@types/react-router": "^5.0.1",
    "@types/react-router-dom": "^4.3.3",
    "@types/react-transition-group": "^2.9.2",
    "@types/styled-components": "^4.1.16",
    "@types/uuid": "^3.4.4",
    "jest": "^24.8.0",
    "serverless": "^1.44.1",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-offline": "^5.3.3",
    "serverless-plugin-typescript": "^1.1.7",
    "serverless-simple-parcel": "^2.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "typescript": "^3.6.0-dev.20190612",
    "typescript-tslint-plugin": "^0.5.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/udia-software/udia-gql.git"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".build"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "handler.ts",
      "src/**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!src/util/initDynamoDB.ts",
      "!.build/**/*"
    ],
    "coverageDirectory": "coverage"
  },
  "license": "Apache-2.0"
}
